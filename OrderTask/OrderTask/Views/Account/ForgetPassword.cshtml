@model ForgetPassword;
@{
    ViewData["Title"] = "ForgetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="ForgetPassword" method="post">
			<div asp-validation-summary="All"></div>

			<input class="text" type="text" asp-for="Email" placeholder="Email" required="">

			
			<input type="submit" value="Send Reset Password">
		</form>
		<p> <a asp-action="Login"> Cancel</a></p>
		<br />
		
	</div>
</div>
@section Scripts {
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
}
